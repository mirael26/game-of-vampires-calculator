{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/intimacy",
      "dest": "/intimacy.html"
    },
    {
      "src": "/attraction",
      "dest": "/attraction.html"
    },
    {
      "src": "/(.*)",
      "dest": "/intimacy.html"
    }
  ]
}
